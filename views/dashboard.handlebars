
<button id="createPost">Create a Post</button>
{{#if posts}}
<ol id="postList">
{{#each posts}}
        
<li id="{{this.id}}">
    <h2>{{this.title}}</h2>
    <a href="/posts/{{this.id}}">Link to post</a>
    <p>{{this.content}}</p>
    <button class="deletePost">Delete Post</button>
</li>
{{/each}}

</ol>
{{/if}}



<script src="/js/dashboard.js"></script>